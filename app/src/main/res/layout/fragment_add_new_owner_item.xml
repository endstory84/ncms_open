<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/addItemView"
            android:orientation="vertical"
            tools:ignore="MissingConstraints">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/addBuldLinkListTitleView"
                android:orientation="horizontal">

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/wtnncCommNum"
                    android:layout_weight="1"
                    android:background="@color/wtnncTextCr"
                    android:layout_margin="2dp"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="15sp"
                    app:autoSizeMinTextSize="9sp" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/indvdlGrpTy"
                    android:layout_weight="1"
                    android:background="@color/wtnncTextCr"
                    android:layout_margin="2dp"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="15sp"
                    app:autoSizeMinTextSize="9sp" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/ownerName"
                    android:layout_weight="2"
                    android:background="@color/wtnncTextCr"
                    android:layout_margin="2dp"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="15sp"
                    app:autoSizeMinTextSize="9sp" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/ownerSameName"
                    android:layout_weight="1"
                    android:background="@color/wtnncTextCr"
                    android:layout_margin="2dp"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="15sp"
                    app:autoSizeMinTextSize="9sp" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/landGovRgistAddr"
                    android:layout_weight="6"
                    android:background="@color/wtnncTextCr"
                    android:layout_margin="2dp"
                    app:autoSizeTextType="uniform"
                    app:autoSizeMaxTextSize="15sp"
                    app:autoSizeMinTextSize="9sp" />
        </LinearLayout>

        <LinearLayout
                android:id="@+id/addOwnerRelateListBaseViewGroup"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                app:layout_constraintTop_toBottomOf="@+id/addBuldLinkListTitleView"
                android:orientation="horizontal">

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:id="@+id/newOwnerNoText"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/thingCn"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/view_line2"
                    android:layout_margin="2dp" />

            <LinearLayout
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_weight="10">

                <LinearLayout
                        android:id="@+id/addOwnerBaseViewGroupSub1"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal">

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerDivisionText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:background="@drawable/view_line2" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerNameText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:layout_weight="2"
                            android:background="@drawable/view_line2" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerSameNameText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:background="@drawable/view_line2" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerRgistAddrText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:gravity="center"
                            android:layout_weight="6"
                            android:textAppearance="@style/TextAppearance.AppCompat.Large"
                            android:background="@drawable/view_line2" />

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/addOwnerBaseViewGroupSub2"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal">

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/landPosesnQota"
                            android:layout_weight="1"
                            android:background="@color/wtnncTextCr"
                            android:layout_margin="2dp"
                            app:autoSizeTextType="uniform"
                            app:autoSizeMaxTextSize="15sp"
                            app:autoSizeMinTextSize="9sp" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/landUnDcsnOwnerAt"
                            android:layout_weight="1"
                            android:background="@color/wtnncTextCr"
                            android:layout_margin="2dp"
                            app:autoSizeTextType="uniform"
                            app:autoSizeMaxTextSize="15sp"
                            app:autoSizeMinTextSize="9sp" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/ownerCprNo"
                            android:layout_weight="3"
                            android:background="@color/wtnncTextCr"
                            android:layout_margin="2dp"
                            app:autoSizeTextType="uniform"
                            app:autoSizeMaxTextSize="15sp"
                            app:autoSizeMinTextSize="9sp" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/landDelvyAddr"
                            android:layout_weight="7"
                            android:background="@color/wtnncTextCr"
                            android:layout_margin="2dp"
                            app:autoSizeTextType="uniform"
                            app:autoSizeMaxTextSize="15sp"
                            app:autoSizeMinTextSize="9sp" />
                </LinearLayout>

                <LinearLayout
                        android:id="@+id/addOwnerBaseViewGroupSub3"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal">

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerPosesnQota"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:background="@drawable/view_line2" />

                    <!--                <EditText-->
                    <!--                        style="@style/Widget.NCMS.WtnccValueEditText"-->
                    <!--                        android:id="@+id/newOwnerPosesnQota"-->
                    <!--                        android:layout_weight="1" />-->
                    <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:background="@drawable/view_line2"
                            android:layout_margin="2dp">

                        <CheckBox
                                android:id="@+id/newOwnerUnDcsnOwnerAt"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:gravity="center"
                                android:layout_margin="2dp" />
                    </LinearLayout>

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerCrpNoText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:layout_weight="3"
                            android:background="@drawable/view_line2" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccValueTextView"
                            android:id="@+id/newOwnerDelvyAddrText"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:text="@string/thingCn"
                            android:gravity="center"
                            android:layout_weight="7"
                            android:background="@drawable/view_line2" />
                    <!--                <EditText-->
                    <!--                        style="@style/Widget.NCMS.WtnccValueEditText"-->
                    <!--                        android:id="@+id/newOwnerDelvyAddrText"-->
                    <!--                        android:layout_weight="7" />-->

                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>