<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_thing_dialog_radius"
        android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:id="@+id/constraintLayout6">

        <TextView
                android:fontFamily="@font/nsquareextrabold"
                android:autoSizeMaxTextSize="32sp"
                android:textSize="28sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@string/wtnncCommAddSelectRelate"
                android:textColor="?android:attr/textColorPrimary"
                android:id="@+id/textView2" />

        <!--        <ImageView-->
        <!--                android:id="@+id/thingRegstrDialogExitBtn"-->
        <!--                android:layout_width="30dp"-->
        <!--                android:layout_height="30dp"-->
        <!--                android:layout_marginEnd="20dp"-->
        <!--                android:src="@drawable/ic_cancel"-->
        <!--                app:layout_constraintTop_toTopOf="parent"-->
        <!--                app:layout_constraintBottom_toBottomOf="parent"-->
        <!--                android:contentDescription="닫기"-->
        <!--                app:layout_constraintEnd_toEndOf="parent" />-->
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout6"
            android:id="@+id/addBuldLinkListTitleView">

        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerDivision"
                android:layout_weight="2"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerName"
                android:layout_weight="2"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerSameName"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerCprNo"
                android:layout_weight="3"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerDelvyAddr"
                android:layout_weight="6"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
    </LinearLayout>

    <LinearLayout
            android:id="@+id/addOwnerRelateListBaseViewGroup"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            app:layout_constraintTop_toBottomOf="@+id/addBuldLinkListTitleView"
            android:orientation="horizontal" >
        <TextView
                android:id="@+id/ownerDivisionText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="2"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                android:id="@+id/ownerNameText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="2"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />

        <TextView
                android:id="@+id/ownerSameNameText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                android:id="@+id/ownerCrpNoText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="3"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                android:id="@+id/ownerDelvyAddrText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="6"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
    </LinearLayout>

    <LinearLayout
            android:id="@+id/addOwnerRelateListBaseViewGroup2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            app:layout_constraintTop_toBottomOf="@+id/addOwnerRelateListBaseViewGroup"
            android:orientation="horizontal" >
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerBankYn"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/spotNm"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerPcnRightRelate"
                android:layout_weight="3"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />

    </LinearLayout>
    <LinearLayout
            android:id="@+id/addOwnerRelateListBaseViewGroup3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            app:layout_constraintTop_toBottomOf="@+id/addOwnerRelateListBaseViewGroup2"
            android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/view_line2"
            android:layout_margin="2dp">

            <CheckBox
                    android:id="@+id/addOwnerRelateBankAt"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:gravity="center"
                    android:layout_margin="2dp" />
        </LinearLayout>
        <EditText
                style="@style/Widget.NCMS.WtnccValueEditText"
                android:id="@+id/ownerRelateBankSpotNm"
                android:background="@drawable/view_line2"
                android:layout_weight="1" />
        <EditText
                style="@style/Widget.NCMS.WtnccValueEditText"
                android:id="@+id/ownerPcnRightRelate"
                android:background="@drawable/view_line2"
                android:layout_weight="3" />
    </LinearLayout>



    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/addOwnerRelateListBaseViewGroup3">

        <Button
                style="@style/Widget.NCMS.Alert.Button.Negative"
                android:id="@+id/cancelBtn"
                android:text="@string/wtnncCommCancel"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />
        <Button
                style="@style/Widget.NCMS.Alert.Button.Positive"
                android:id="@+id/selectInputBtn"
                android:text="@string/wtnncCommInput"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>