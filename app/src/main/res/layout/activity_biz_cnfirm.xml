<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BizCnfirmActivity">

    <!-- 용지도 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmLotMap"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_1"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_lotmap"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
        />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- 토지 & 지장물 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm2"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/layoutBizCnfirm1">

        <com.google.android.material.button.MaterialButton
                style="@style/Widget.NCMS.CnfirmButton"
                android:id="@+id/buttonBizCnfirmLad"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_2"
                app:icon="@drawable/ic_biz_lad"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBizCnfirmThing"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmThing"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_3"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_thing"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonBizCnfirmLad"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- 영업ㆍ축산업ㆍ잠업 & 농업 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm3"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/layoutBizCnfirm2">

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmBsn"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_4"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_bsn"
                android:maxLines="1"
                android:ellipsize="end"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBizCnfirmFarm"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmFarm"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_5"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_farm"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonBizCnfirmBsn"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- 광업권 & 어업권 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm4"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/layoutBizCnfirm3">

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmMnidst"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_6"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_minrgt"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBizCnfirmFshr"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmFshr"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_7"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_fyhts"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonBizCnfirmMnidst"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- 거주자 & 분묘 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm5"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/layoutBizCnfirm4">

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmResidnt"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_8"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_residnt"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBizCnfirmTomb"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmTomb"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_9"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_tomb"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonBizCnfirmResidnt"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- (임시) 잔여지 -->
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutBizCnfirm6"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/layoutBizCnfirm5">

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmRestLAD"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_98"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_residnt"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBizCnfirmRestThing"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBizCnfirmRestThing"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:text="@string/drawer_menu_99"
                style="@style/Widget.NCMS.CnfirmButton"
                app:icon="@drawable/ic_biz_tomb"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonBizCnfirmRestLAD"
                app:layout_constraintTop_toTopOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>