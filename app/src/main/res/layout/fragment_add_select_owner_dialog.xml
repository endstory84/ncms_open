<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_thing_dialog_radius"
        android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:id="@+id/constraintLayout6">

        <TextView
                android:fontFamily="@font/nsquareextrabold"
                android:autoSizeMaxTextSize="32sp"
                android:textSize="28sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@string/wtnncCommAddOwner"
                android:textColor="?android:attr/textColorPrimary"
                android:id="@+id/textView2" />

        <!--        <ImageView-->
        <!--                android:id="@+id/thingRegstrDialogExitBtn"-->
        <!--                android:layout_width="30dp"-->
        <!--                android:layout_height="30dp"-->
        <!--                android:layout_marginEnd="20dp"-->
        <!--                android:src="@drawable/ic_cancel"-->
        <!--                app:layout_constraintTop_toTopOf="parent"-->
        <!--                app:layout_constraintBottom_toBottomOf="parent"-->
        <!--                android:contentDescription="닫기"-->
        <!--                app:layout_constraintEnd_toEndOf="parent" />-->
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
            android:id="@+id/addBuldLinkListBaseViewGroup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout6"
            android:orientation="horizontal" >
        <ListView
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="410dp"
                android:id="@+id/ownerListInfoView" />
    </LinearLayout>


    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/addBuldLinkListBaseViewGroup"
            android:id="@+id/addBuldLinkListTitleView">

        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/wtnncCommNum"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/wtnncCommDivice"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp"/>
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerName"
                android:layout_weight="2"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/ownerSameName"
                android:layout_weight="1"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />

        <TextView
                style="@style/Widget.NCMS.WtnccLabelTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/landGovRgistAddr"
                android:layout_weight="6"
                android:background="@color/wtnncTextCr"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
    </LinearLayout>

    <LinearLayout
            android:id="@+id/addOwnerRelateListBaseViewGroup"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            app:layout_constraintTop_toBottomOf="@+id/addBuldLinkListTitleView"
            android:orientation="horizontal" >
        <TextView
                android:id="@+id/ownerNoText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="@string/thingCn"
                android:gravity="center"
                android:layout_weight="1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:background="@drawable/view_line2"
                android:layout_margin="2dp"
                app:autoSizeTextType="uniform"
                app:autoSizeMaxTextSize="15sp"
                app:autoSizeMinTextSize="9sp" />
        <LinearLayout
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="10">

            <LinearLayout
                android:id="@+id/addOwnerBaseViewGroupSub1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                        android:id="@+id/ownerDivisionText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/thingCn"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
                <TextView
                        android:id="@+id/ownerNameText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/thingCn"
                        android:gravity="center"
                        android:layout_weight="2"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />

                <TextView
                        android:id="@+id/ownerSameNameText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/thingCn"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />

                <EditText
                        style="@style/Widget.NCMS.WtnccValueEditText"
                        android:id="@+id/addOwnerRgistAddrText"
                        android:background="@drawable/view_line2"
                        android:layout_weight="6" />


            </LinearLayout>
            <LinearLayout
                    android:id="@+id/addOwnerBaseViewGroupSub2"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
            >
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/landPosesnQota"
                        android:layout_weight="2"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/landUnDcsnOwnerAt"
                        android:layout_weight="1"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/ownerCprNo"
                        android:layout_weight="3"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />

            </LinearLayout>
            <LinearLayout
                    android:id="@+id/addOwnerBaseViewGroupSub3"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
            >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:layout_margin="2dp"
                    android:background="@drawable/view_line2">
                    <EditText
                            style="@style/Widget.NCMS.WtnccValueEditText"
                            android:id="@+id/addOwnerPosesnQotaNum"
                            android:layout_weight="0.8" />
                    <TextView
                            style="@style/Widget.NCMS.WtnccValueEditText"
                            android:text="-"
                            android:layout_weight="0.4"/>
                    <EditText
                            style="@style/Widget.NCMS.WtnccValueEditText"
                            android:id="@+id/addOwnerPosesnQotaDeno"
                            android:layout_weight="0.8" />
                </LinearLayout>


                <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/addOwnerUnDcsnOwnerAt"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />
                </LinearLayout>
                <TextView
                        android:id="@+id/ownerCrpNoText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/thingCn"
                        android:gravity="center"
                        android:layout_weight="3"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />


             </LinearLayout>
            <LinearLayout
                    android:id="@+id/addOwnerBaseViewGroupSub4"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/ownerBankYn"
                        android:layout_weight="1"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/spotNm"
                        android:layout_weight="1"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/landDelvyAddr"
                        android:layout_weight="4"
                        android:background="@color/wtnncTextCr"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
            </LinearLayout>
            <LinearLayout
                    android:id="@+id/addOwnerBaseViewGroupSub5"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">
                <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/addOwnerbaknAtLinear"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/addOwnerGroupBankAt"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp"/>
                </LinearLayout>
                <EditText
                        style="@style/Widget.NCMS.WtnccValueEditText"
                        android:id="@+id/addOwnerGeoupBankSpotNm"
                        android:background="@drawable/view_line2"
                        android:layout_weight="1" />

                <TextView
                        android:id="@+id/ownerDelvyAddrText"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:text="@string/thingCn"
                        android:gravity="center"
                        android:layout_weight="4"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:background="@drawable/view_line2"
                        android:layout_margin="2dp"
                        app:autoSizeTextType="uniform"
                        app:autoSizeMaxTextSize="15sp"
                        app:autoSizeMinTextSize="9sp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/addOwnerRelateListBaseViewGroup">

        <Button
                style="@style/Widget.NCMS.Alert.Button.Negative"
                android:id="@+id/cancelBtn"
                android:text="@string/wtnncCommCancel"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />
        <Button
                style="@style/Widget.NCMS.Alert.Button.Positive"
                android:id="@+id/selectInputBtn"
                android:text="@string/wtnncCommInput"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>