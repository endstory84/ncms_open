<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/bizListJibunLayout">

        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="50dp">

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1.5"
                    android:text="@string/bizJibunSearchNo" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="3"
                    android:text="@string/bizJibunLegaldongNm" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1.5"
                    android:text="@string/bizJibunBgnnLnm" />
            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1.5"
                    android:text="@string/bizJibunIncrprLnm" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1.5"
                    android:text="@string/bizJibunBgnnAr" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1.5"
                    android:text="@string/bizJibunIncrprAr" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="2"
                    android:text="@string/bizJibunGobuLndcgrNm" />

            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="3"
                    android:text="@string/bizJibunOwnerName" />
            <TextView
                    style="@style/Widget.NCMS.WtnccLabelTextView"
                    android:layout_weight="1"
                    android:text="@string/bizJibunDcnsAt" />

        </LinearLayout>

        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="50dp">

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1.5"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunSearchNoTxt"
                    android:text="@string/bizJibunSearchNo" />

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="3"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunLegaldongNmTxt"
                    android:text="@string/bizJibunLegaldongNm" />

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1.5"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunBgnnLnmTxt"
                    android:text="@string/bizJibunBgnnLnm" />
            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1.5"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunIncrprLnmTxt"
                    android:text="@string/bizJibunIncrprLnm" />
            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1.5"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunBgnnArTxt"
                    android:text="@string/bizJibunBgnnAr" />

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1.5"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunIncrprArTxt"
                    android:text="@string/bizJibunIncrprAr" />

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="2"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunGobuLndcgrNmTxt"
                    android:text="@string/bizJibunGobuLndcgrNm" />

            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="3"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunOwnerNameTxt"
                    android:text="@string/bizJibunOwnerName" />
            <TextView
                    style="@style/Widget.NCMS.WtnccValueTextView"
                    android:layout_weight="1"
                    android:textColor="?android:attr/textColorPrimary"
                    android:id="@+id/bizJibunDcnsAtTxt"
                    android:text="N" />


        </LinearLayout>

    </LinearLayout>


</LinearLayout>