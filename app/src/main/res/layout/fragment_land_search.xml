<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="30dp"
            android:id="@+id/constraint1">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

            <LinearLayout
                    android:id="@+id/landSearchFragmentLl1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchNo"
                        android:text="@string/wtnncCommNum" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchNoText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchLocation"
                        android:text="@string/wtnncCommLocplc" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchLocationText" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landSearchFragmentLl2"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchBgnnLnm"
                        android:text="@string/wtnncCommOriginalLnm" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchincrprLnm"
                        android:text="@string/wtnncCommincrprLnm" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchNomination"
                        android:text="@string/wtnncCommLndcgr" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchRelatedLnm"
                        android:text="@string/wtnncCommRelateLnm" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchBgnnAr"
                        android:text="@string/wtnncCommAllAr" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchIncrprAr"
                        android:text="@string/wtnncCommIncrprAr" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landSearchFragmentLl3"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchBgnnLnmText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchincrprLnmText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchNominationText" />

                <EditText
                        style="@style/Widget.NCMS.WtnccValueEditText"
                        android:layout_weight="1"
                        android:hint="@string/landInfoRelatedLnmText"
                        android:id="@+id/landSearchRelatedLnmText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchBgnnArText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchIncrprArText" />
            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/landSearchFragmentLl6"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="200dp">

                <LinearLayout
                        android:id="@+id/landSearchFragmentLl7"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/landSearchFragmentLl8"
                        app:layout_constraintHorizontal_weight="1"
                        android:layout_margin="2dp">

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:id="@+id/landSearchRealLndcgrCl"
                            android:text="@string/landSearchRealLndcgrCl" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:id="@+id/landSearchRealLndcgrCn"
                            android:text="@string/landSearchRealLndcgrCn" />

                    <TextView
                            style="@style/Widget.NCMS.WtnccLabelTextView"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:id="@+id/landSearchRealLndcgrAr"
                            android:text="@string/landSearchRealLndcgrAr" />

                </LinearLayout>

                <kr.or.kreb.ncms.mobile.fragment.NestedScrollableHost
                        android:id="@+id/landSearchFragmentLl8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        app:layout_constrainedHeight="true"
                        app:layout_constraintStart_toEndOf="@id/landSearchFragmentLl7"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_weight="3"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent">

                    <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/landSearchRealUse"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/white"
                            app:layout_behavior="@string/appbar_scrolling_view_behavior" />

                </kr.or.kreb.ncms.mobile.fragment.NestedScrollableHost>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                    android:id="@+id/landSearchFragmentLl4"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchaNrfrstAt"
                        android:text="@string/landSearchaNrfrstAt" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchClvtAt"
                        android:text="@string/landSearchClvtAt" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchBuildAt"
                        android:text="@string/landSearchBuildAt" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchPlotAt"
                        android:text="@string/landSearchPlotAt" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landRewardAt"
                        android:text="@string/landRewardAt" />


            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landSearchFragmentLl5"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:baselineAligned="true">

                <LinearLayout
                        android:id="@+id/landSearchaNrfrstAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchaNrfrstAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />
                </LinearLayout>

                <LinearLayout
                        android:id="@+id/landSearchClvtAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchClvtAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/landSearchBuildAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchBuildAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/landSearchPlotAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchPlotAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>
                <LinearLayout
                        android:id="@+id/landSearchRewardAtLi"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchRwTrgetAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>



            </LinearLayout>
            <LinearLayout
                    android:id="@+id/landSearchFragmentLl10"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchSttusMesrAt"
                        android:text="@string/landSearchSttusMesrAt" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchPartitnTrgetAt"
                        android:text="@string/landSearchPartitnTrgetAt" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchSpclLadCl"
                        android:text="@string/landSearchSpclLadCl" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchSpclLadCN"
                        android:text="@string/landSearchSpclLadCn" />
                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landSearchownerCnfirmBasisCl"
                        android:text="@string/landSearchOwnerCnfirmBasisCl" />

            </LinearLayout>
            <LinearLayout
                    android:id="@+id/landSearchFragmentLl11"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                <LinearLayout
                        android:id="@+id/landSearchSttusMesrAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchSttusMesrAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>
                <LinearLayout
                        android:id="@+id/landSearchPartitnTrgetAtLl"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="@drawable/textview_border"
                        android:layout_margin="2dp">

                    <CheckBox
                            android:id="@+id/landSearchPartitnTrgetAtChk"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:layout_margin="2dp" />

                </LinearLayout>
                <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_margin="2dp"
                        android:orientation="vertical"
                        android:background="@drawable/textview_border">

                    <Spinner
                            android:id="@+id/landSearchSpclLadClAtChk"
                            style="@style/Widget.NCMS.Spinner"/>

                </LinearLayout>
                <EditText
                        style="@style/Widget.NCMS.WtnccValueEditText"
                        android:id="@+id/landSearchSpclLadCnEditText"
                        android:inputType="number"
                        android:layout_weight="1" />
                <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_margin="2dp"
                        android:orientation="vertical"
                        android:background="@drawable/textview_border">

                    <Spinner
                            android:id="@+id/landSearchOwnerCnfirmBasisClChk"
                            style="@style/Widget.NCMS.Spinner"/>

                </LinearLayout>


            </LinearLayout>


            <include
                    android:id="@+id/include_wtnncs_lad_camera"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/include_wtnnc_camera" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>