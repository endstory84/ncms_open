<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:background="@color/white">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                android:orientation="vertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoNo"
                        android:text="@string/wtnncCommNum" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoNoText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoLocation"
                        android:text="@string/wtnncCommLocplc" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoLocationText" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl2"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoBgnnLnm"
                        android:text="@string/wtnncCommOriginalLnm" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoincrprLnm"
                        android:text="@string/wtnncCommincrprLnm" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoNomination"
                        android:text="@string/wtnncCommLndcgr" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoRelatedLnm"
                        android:text="@string/wtnncCommRelateLnm" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl3"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoBgnnLnmText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoincrprLnmText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoNominationText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoRelatedLnmText" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl4"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoBgnnAr"
                        android:text="@string/wtnncCommAllAr" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoIncrprAr"
                        android:text="@string/wtnncCommIncrprAr" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoSpfc"
                        android:text="@string/landInfoSpfc" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl5"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoBgnnArText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoIncrprArText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoSpfcText" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl6"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoOwners"
                        android:text="@string/landInfoOwners" />

                <TextView
                        style="@style/Widget.NCMS.WtnccLabelTextView"
                        android:layout_weight="1"
                        android:id="@+id/lanfInfoRelation"
                        android:text="@string/lanfInfoRelation" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl7"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/landInfoOwnersText" />

                <TextView
                        style="@style/Widget.NCMS.WtnccValueTextView"
                        android:layout_weight="1"
                        android:id="@+id/lanfInfoRelationText" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl8"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoLedgerBtn"
                        android:text="@string/wtnncCommInfoLedger" />

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoRegisteredBtn"
                        android:text="@string/wtnncCommInfoRegistered" />

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoRegistMap"
                        android:text="@string/wtnncCommLandRegiMap" />

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoUsePlanBtn"
                        android:text="@string/landInfoUsePlan" />

            </LinearLayout>

            <LinearLayout
                    android:id="@+id/landInfoFragmentLl9"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoPreviousCompensateBtn"
                        android:text="@string/landInfoPreviousCompensate" />

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoFeedChannelBtn"
                        android:text="@string/landInfoFeedChannel" />

                <Button
                        style="@style/Widget.NCMS.WtnccPopupButton"
                        android:id="@+id/landInfoSurveyResultMap"
                        android:text="@string/wtnncCommSurveyResultMap" />
            </LinearLayout>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>