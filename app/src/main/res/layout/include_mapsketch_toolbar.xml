<merge xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <!-- 도형 스케치 -->
    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonPolygon"
            android:text="@string/fab_toolbar_9"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_9" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonCancel"
            android:text="@string/fab_toolbar_8"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_8" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonRemove"
            android:text="@string/fab_toolbar_7"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_7" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonModify"
            android:text="@string/fab_toolbar_6"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_6" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        style="@style/Widget.NCMS.FloatingActionButton"
        android:id="@+id/floatingActionButtonLine"
        android:text="@string/fab_toolbar_5"
        app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
        app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
        app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
        app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
        android:tooltipText="@string/fab_toolbar_5" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        style="@style/Widget.NCMS.FloatingActionButton"
        android:id="@+id/floatingActionButtonPoint"
        android:text="@string/fab_toolbar_4"
        app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
        app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
        app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
        app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
        android:tooltipText="@string/fab_toolbar_4" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonRedo"
            android:text="@string/fab_toolbar_3"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_3" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonUndo"
            android:text="@string/fab_toolbar_2"
            app:layout_constraintTop_toTopOf="@id/floatingActionButtonToolbar"
            app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonToolbar"
            app:layout_constraintStart_toStartOf="@id/floatingActionButtonToolbar"
            app:layout_constraintEnd_toEndOf="@id/floatingActionButtonToolbar"
            android:tooltipText="@string/fab_toolbar_2" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            style="@style/Widget.NCMS.FloatingActionButton"
            android:id="@+id/floatingActionButtonToolbar"
            app:icon="@drawable/ic_baseline_add_24"
            app:backgroundTint="@color/colorAccent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="10dp"
            android:layout_margin="8dp"
            android:tooltipText="@string/fab_toolbar_1" />


</merge>